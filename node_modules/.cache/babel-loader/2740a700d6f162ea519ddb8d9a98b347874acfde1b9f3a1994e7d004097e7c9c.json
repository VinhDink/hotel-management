{"ast":null,"code":"import { defineComponent, ref } from 'vue';\nimport axios from '../config';\nexport default defineComponent({\n  name: 'UserListView',\n  setup() {\n    const users = ref([]);\n\n    /**\n     * Get all users\n     * \n     * @return {void}\n     */\n    const getUser = async () => {\n      const response = await axios.get('userlist');\n      users.value = response.data;\n    };\n\n    /**\n     * Delete a user\n     * \n     * @param {int} id \n     * \n     * @return {void}\n     */\n    const deleteUser = async id => {\n      console.log(id);\n      try {\n        const response = await axios.delete('userlist/del', {\n          data: {\n            id: id\n          }\n        });\n        console.log(response.data);\n        getUser();\n      } catch (error) {\n        console.error(error.response.data);\n      }\n    };\n    getUser();\n    return {\n      users,\n      deleteUser\n    };\n  }\n});","map":{"version":3,"names":["defineComponent","ref","axios","name","setup","users","getUser","response","get","value","data","deleteUser","id","console","log","delete","error"],"sources":["/Users/vinh/Documents/Kiai/abc_hotel/src/views/UserListView.vue"],"sourcesContent":["<template>\n  <div>\n    <div>\n      <h1>List of user:</h1>\n    </div>\n    <div>\n      <div class=\"table-box\">\n        <table class=\"table\">\n          <tr>\n            <th scope=\"col\">ID</th>\n          <th scope=\"col\">Username</th>\n          <th scope=\"col\">Role</th>\n          <th scope=\"col\">Actions</th>\n          </tr>\n          <tr v-for=\"booking in bookings\" :key=\"booking.id\">\n            <td>{{ guestInfo.id }}</td>\n            <td>{{ guestInfo.guest_name }}</td>\n            <td>{{ guestInfo.guest_number }}</td>\n            <td>{{ guestInfo.room_id }}</td>\n            <td>{{ guestInfo.checkin.total_price }}</td>\n          </tr>\n        </table>\n      </div>\n    </div>\n    <!-- <table class=\"table table-striped\">\n      <thead>\n        <tr>\n          <th scope=\"col\">ID</th>\n          <th scope=\"col\">Username</th>\n          <th scope=\"col\">Role</th>\n          <th scope=\"col\">Actions</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"user in users\" :key=\"user.id\">\n          <td>{{ user.id }}</td>\n          <td>{{ user.username }}</td>\n          <td>{{ user.role }}</td>\n          <td>\n            <button @click=\"deleteUser(user.id)\" class=\"btn btn-danger\">\n              <i class=\"bi bi-trash\"></i> Delete\n            </button>\n          </td>\n        </tr>\n      </tbody>\n    </table> -->\n  </div>\n</template>\n\n<script>\nimport { defineComponent, ref } from 'vue'\nimport axios from '../config'\n\nexport default defineComponent({\n  name: 'UserListView',\n\n  setup() {\n    const users = ref([]);\n\n    /**\n     * Get all users\n     * \n     * @return {void}\n     */\n    const getUser = async () => {\n      const response = await axios.get('userlist')\n      users.value = response.data\n    }\n\n    /**\n     * Delete a user\n     * \n     * @param {int} id \n     * \n     * @return {void}\n     */\n    const deleteUser = async (id) => {\n      console.log(id)\n      try {\n        const response = await axios.delete('userlist/del', {\n          data: {\n            id: id,\n          },\n        })\n        console.log(response.data)\n        getUser()\n      } catch (error) {\n        console.error(error.response.data)\n      }\n    }\n    getUser()\n\n    return {\n      users,\n      deleteUser\n    }\n  },\n})\n</script>\n"],"mappings":"AAkDA,SAASA,eAAe,EAAEC,GAAE,QAAS,KAAI;AACzC,OAAOC,KAAI,MAAO,WAAU;AAE5B,eAAeF,eAAe,CAAC;EAC7BG,IAAI,EAAE,cAAc;EAEpBC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIJ,GAAG,CAAC,EAAE,CAAC;;IAErB;;;;;IAKA,MAAMK,OAAM,GAAI,MAAAA,CAAA,KAAY;MAC1B,MAAMC,QAAO,GAAI,MAAML,KAAK,CAACM,GAAG,CAAC,UAAU;MAC3CH,KAAK,CAACI,KAAI,GAAIF,QAAQ,CAACG,IAAG;IAC5B;;IAEA;;;;;;;IAOA,MAAMC,UAAS,GAAI,MAAOC,EAAE,IAAK;MAC/BC,OAAO,CAACC,GAAG,CAACF,EAAE