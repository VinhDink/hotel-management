 'vue'\nimport axios from '../config'\n\nexport default {\n    name: 'TestingView',\n    components: {\n        DataTable\n    },\n\n    setup() {\n        const tableData = ref({})\n        const roomInfo = ref({})\n        const modalStatus = ref(false)\n        const tableTitles = ref([\n            'Room ID',\n            'Room Name',\n            'Type',\n            'Actions',\n        ])\n        const getRooms = async () => {\n            let results = await axios.get('room')\n            tableData.value = results.data\n            console.log(tableData.value)\n        }\n\n        const closeModal = async () => {\n            document.getElementById('detail-modal').style.display = 'none'\n        }\n\n        const viewRoomDetail = (id) => {\n            console.log('hello')\n\n          \n            document.getElementById('detail-modal').style.display = 'block'\n        }\n\n        getRooms()\n        return {\n            tableData,\n            tableTitles,\n            viewRoomDetail,\n            closeModal,\n            roomInfo,\n            modalStatus\n        }\n    }\n}\n</script>\n"],"mappings":"AA0CA,OAAOA,SAAQ,MAAO,6BAA4B;AAClD,SAASC,GAAE,QAAS,KAAI;AACxB,OAAOC,KAAI,MAAO,WAAU;AAE5B,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACRJ;EACJ,CAAC;EAEDK,KAAKA,CAAA,EAAG;IACJ,MAAMC,SAAQ,GAAIL,GAAG,CAAC,CAAC,CAAC;IACxB,MAAMM,QAAO,GAAIN,GAAG,CAAC,CAAC,CAAC;IACvB,MAAMO,WAAU,GAAIP,GAAG,CAAC,KAAK;IAC7B,MAAMQ,WAAU,GAAIR,GAAG,CAAC,CACpB,SAAS,EACT,WAAW,EACX,MAAM,EACN,SAAS,CACZ;IACD,MAAMS,QAAO,GAAI,MAAAA,CAAA,KAAY;MACzB,IAAIC,OAAM,GAAI,MAAMT,KAAK,CAACU,GAAG,CAAC,MAAM;MACpCN,SAAS,CAACO,KAAI,GAAIF,OAAO,CAACG,IAAG;MAC7BC,OAAO,CAACC,GAAG,CAACV,SAAS,CAACO,KAAK;IAC/B;IAEA,MAAMI,UAAS,GAAI,MAAAA,CAAA,KAAY;MAC3BC,QAAQ,CAACC,cAAc,CAAC,cAAc,CAAC,CAACC,KAAK,CAACC,OAAM,GAAI,MAAK;IACjE;IAEA,MAAMC,cAAa,GAAKC,EAAE,IAAK;MAC3BR,OAAO,CAACC,GAAG,CAAC,OAAO;MAGnBE,QAAQ,CAACC,cAAc,CAAC,cAAc,CAAC,CAACC,KAAK,CAACC,OAAM,GAAI,OAAM;IAClE;IAEAX,QAAQ,CAAC;IACT,OAAO;MACHJ,SAAS;MACTG,WAAW;MA