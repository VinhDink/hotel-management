{"ast":null,"code":"import DataTable from '../components/DataTable.vue';\nimport { ref } from 'vue';\nimport axios from '../config';\nimport DetailInfo from '../components/DetailInfo.vue';\nexport default {\n  name: 'TestingView',\n  components: {\n    DataTable,\n    DetailInfo\n  },\n  setup() {\n    const tableData = ref({});\n    const roomInfo = ref({});\n    const modalStatus = ref(false);\n    const tableTitles = ref(['Room ID', 'Room Name', 'Type', 'Actions']);\n    const getRooms = async () => {\n      let results = await axios.get('room');\n      tableData.value = results.data;\n    };\n    const closeModal = async () => {\n      document.getElementById('detail-modal').style.display = 'none';\n    };\n    const viewRoomDetail = async id => {\n      let response = await axios.post('room-detail', {\n        id: id\n      });\n      roomInfo.value = {\n        'Id': response.data.id,\n        'Name': response.data.name,\n        'Type': response.data.type,\n        'Hour price': response.data.hour_price,\n        'Day price': response.data.day_price,\n        'Status': response.data.status,\n        'Size': response.data.size,\n        'Balcony': response.data.balcony,\n        'View': response.data.view,\n        'Smoking': response.data.smoking,\n        'Floor': response.data.floor,\n        'Bathtub': response.data.bathtub\n      };\n      console.log(response.data.id);\n      document.getElementById('detail-modal').style.display = 'block';\n    };\n    getRooms();\n    return {\n      tableData,\n      tableTitles,\n      viewRoomDetail,\n      closeModal,\n      roomInfo,\n      modalStatus\n    };\n  }\n};","map":{"version":3,"names":["DataTable","ref","axios","DetailInfo","name","components","setup","tableData","roomInfo","modalStatus","tableTitles","getRooms","results","get","value","data","closeModal","document","getElementById","style","display","viewRoomDetail","id","response","post","type","hour_price","day_price","status","size","balcony","view","smoking","floor","bathtub","console","log"],"sources":["/Users/vinh/Documents/Kiai/abc_hotel/src/views/TestingView.vue"],"sourcesContent":["<template>\n    <div>\n        <DataTable :tableDatas=\"tableData\" :tableTitles=\"tableTitles\" :actionModify=\"true\" :actionDetail=\"true\"\n            :function=\"viewRoomDetail\"></DataTable>\n            <div>\n                <DetailInfo :datas></DetailInfo>\n            </div>\n    </div>\n</template>\n\n<script>\nimport DataTable from '../components/DataTable.vue'\nimport { ref } from 'vue'\nimport axios from '../config'\nimport DetailInfo from '../components/DetailInfo.vue'\n\nexport default {\n    name: 'TestingView',\n    components: {\n        DataTable,\n        DetailInfo\n    },\n\n    setup() {\n        const tableData = ref({})\n        const roomInfo = ref({})\n        const modalStatus = ref(false)\n        const tableTitles = ref([\n            'Room ID',\n            'Room Name',\n            'Type',\n            'Actions',\n        ])\n        const getRooms = async () => {\n            let results = await axios.get('room')\n            tableData.value = results.data\n        }\n\n        const closeModal = async () => {\n            document.getElementById('detail-modal').style.display = 'none'\n        }\n\n        const viewRoomDetail = async (id) => {\n            let response = await axios.post('room-detail', {\n                id: id\n            })\n            roomInfo.value = {\n                'Id': response.data.id,\n                'Name': response.data.name,\n                'Type': response.data.type,\n                'Hour price': response.data.hour_price,\n                'Day price': response.data.day_price,\n                'Status': response.data.status,\n                'Size': response.data.size,\n                'Balcony': response.data.balcony,\n                'View': response.data.view,\n                'Smoking': response.data.smoking,\n                'Floor': response.data.floor,\n                'Bathtub': response.data.bathtub,\n            }\n            console.log(response.data.id)\n\n            document.getElementById('detail-modal').style.display = 'block'\n        }\n\n        getRooms()\n        return {\n            tableData,\n            tableTitles,\n            viewRoomDetail,\n            closeModal,\n            roomInfo,\n            modalStatus,\n        }\n    }\n}\n</script>\n"],"mappings":"AAWA,OAAOA,SAAQ,MAAO,6BAA4B;AAClD,SAASC,GAAE,QAAS,KAAI;AACxB,OAAOC,KAAI,MAAO,WAAU;AAC5B,OAAOC,UAAS,MAAO,8BAA6B;AAEpD,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACRL,SAAS;IACTG;EACJ,CAAC;EAEDG,KAAKA,CAAA,EAAG;IACJ,MAAMC,SAAQ,GAAIN,GAAG,CAAC,CAAC,CAAC;IACxB,MAAMO,QAAO,GAAIP,GAAG,CAAC,CAAC,CAAC;IACvB,MAAMQ,WAAU,GAAIR,GAAG,CAAC,KAAK;IAC7B,MAAMS,WAAU,GAAIT,GAAG,CAAC,CACpB,SAAS,EACT,WAAW,EACX,MAAM,EACN,SAAS,CACZ;IACD,MAAMU,QAAO,GAAI,MAAAA,CAAA,KAAY;MACzB,IAAIC,OAAM,GAAI,MAAMV,KAAK,CAACW,GAAG,CAAC,MAAM;MACpCN,SAAS,CAACO,KAAI,GAAIF,OAAO,CAACG,IAAG;IACjC;IAEA,MAAMC,UAAS,GAAI,MAAAA,CAAA,KAAY;MAC3BC,QAAQ,CAACC,cAAc,CAAC,cAAc,CAAC,CAACC,KAAK,CAACC,OAAM,GAAI,MAAK;IACjE;IAEA,MAAMC,cAAa,GAAI,MAAOC,EAAE,IAAK;MACjC,IAAIC,QAAO,GAAI,MAAMrB,KAAK,CAACsB,IAAI,CAAC,aAAa,EAAE;QAC3CF,EAAE,EAAEA;MACR,CAAC;MACDd,QAAQ,CAACM,KAAI,GAAI;QACb,IAAI,EAAES,QAAQ,CAACR,IAAI,CAACO,EAAE;QACtB,MAAM,EAAEC,QAAQ,CAACR,IAAI,CAACX,IAAI;QAC1B,MAAM,EAAEmB,QAAQ,CAACR,IAAI,CAACU,IAAI;QAC1B,YAAY,EAAEF,QAAQ,CAACR,IAAI,CAACW,UAAU;QACtC,WAAW,EAAEH,QAAQ,CAACR,IAAI,CAACY,SAAS;QACpC,QAAQ,EAAEJ,QAAQ,CAACR,IAAI,CAACa,MAAM;QAC9B,MAAM,EAAEL,QAAQ,CAACR,IAAI,CAACc,IAAI;QAC1B,SAAS,EAAEN,QAAQ,CAACR,IAAI,CAACe,OAAO;QAChC,MAAM,EAAEP,QAAQ,CAACR,IAAI,CAACgB,IAAI;QAC1B,SAAS,EAAER,QAAQ,CAACR,IAAI,CAACiB,OAAO;QAChC,OAAO,EAAET,QAAQ,CAACR,IAAI,CAACkB,KAAK;QAC5B,SAAS,EAAEV,QAAQ,CAACR,IAAI,CAACmB;MAC7B;MACAC,OAAO,CAACC,GAAG,CAACb,QAAQ,CAACR,IAAI,CAACO,EAAE;MAE5BL,QAAQ,CAACC,cAAc,CAAC,cAAc,CAAC,CAACC,KAAK,CAACC,OAAM,GAAI,OAAM;IAClE;IAEAT,QAAQ,CAAC;IACT,OAAO;MACHJ,SAAS;MACTG,WAAW;MACXW,cAAc;MACdL,UAAU;MA