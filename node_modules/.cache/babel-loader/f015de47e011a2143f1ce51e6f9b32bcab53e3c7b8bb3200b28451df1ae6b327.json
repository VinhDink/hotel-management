{"ast":null,"code":"import { defineComponent, ref } from 'vue';\nimport axios from '../config';\nimport LineChart from '../components/LineChart.vue';\nexport default defineComponent({\n  name: 'DashboardView',\n  components: {\n    LineChart\n  },\n  data() {\n    return {\n      loaded: false,\n      LineChartData: {\n        labels: ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'],\n        datasets: [{\n          data: []\n        }]\n      },\n      LineChartOptions: {\n        plugins: {\n          legend: {\n            display: false\n          }\n        },\n        responsive: true,\n        maintainAspectRatio: false,\n        scales: {\n          y: {\n            min: 0,\n            ticks: {\n              stepSize: 1\n            }\n          }\n        },\n        elements: {\n          point: {\n            backgroundColor: 'red'\n          }\n        }\n      },\n      chartData: {\n        labels: ['January', 'February', 'March', 'April', 'May', 'June', 'July'],\n        datasets: [{\n          label: 'Income',\n          data: [10000, 15000, 20000, 18000, 17000, 25000, 30000],\n          backgroundColor: 'rgba(54, 162, 235, 0.2)',\n          borderColor: 'rgba(54, 162, 235, 1)',\n          borderWidth: 1\n        }]\n      },\n      chartOptions: {\n        scales: {\n          x: {\n            title: {\n              display: true,\n              text: 'Income'\n            },\n            ticks: {\n              beginAtZero: true\n            }\n          },\n          y: {\n            title: {\n              display: true,\n              text: 'Month'\n            }\n          }\n        }\n      }\n    };\n  },\n  setup() {\n    const guestInfos = ref([]);\n    const bookingInfos = ref([]);\n    const getData = async () => {\n      try {\n        let results = await axios.get('dashboard');\n        guestInfos.value = results.data.guestInfos;\n        bookingInfos.value = results.data.bookingInfos;\n      } catch (error) {\n        console.log(error);\n      }\n    };\n    getData();\n    return {\n      guestInfos,\n      bookingInfos\n    };\n  },\n  async mounted() {\n    this.loaded = false;\n    try {\n      const userlist = await axios.get('booking-this-year');\n      this.LineChartData.datasets[0].data = userlist.data;\n      this.loaded = true;\n    } catch (e) {\n      console.error(e);\n    }\n  }\n});","map":{"version":3,"names":["defineComponent","ref","axios","LineChart","name","components","data","loaded","LineChartData","labels","datasets","LineChartOptions","plugins","legend","display","responsive","maintainAspectRatio","scales","y","min","ticks","stepSize","elements","point","backgroundColor","chartData","label","borderColor","borderWidth","chartOptions","x","title","text","beginAtZero","setup","guestInfos","bookingInfos","getData","results","get","value","error","console","log","mounted","userlist","e"],"sources":["/Users/vinh/Documents/Kiai/abc_hotel/src/views/DashboardView.vue"],"sourcesContent":["<template>\n  <div class=\"body\">\n    <div class=\"title-box\">\n      <h1 class=\"title\">Dashboard</h1>\n    </div>\n    <div>\n      <div class=\"table-box\">\n        <table class=\"table\">\n          <tr>\n            <th>Booking ID</th>\n            <th>Guest Name</th>\n            <th>Guest Number</th>\n            <th>Room ID</th>\n            <th>Total Price</th>\n          </tr>\n          <tr v-for=\"guest in guestInfos\" :key=\"guest.id\">\n            <td>{{ guest.id }}</td>\n            <td>{{ guest.guest_name }}</td>\n            <td>{{ guest.guest_number }}</td>\n            <td>{{ guest.room_id }}</td>\n            <td>{{ guest.checkin.total_price }}</td>\n          </tr>\n        </table>\n      </div>\n    </div>\n    <div class=\"summary-box\">\n      <div class=\"summary\">\n        <div class=\"icon-box\">\n          <i class=\"bi bi-cash-stack money-icon\"></i>\n        </div>\n        <div class=\"summary-title\">\n          Today's interest\n        </div>\n        <div class=\"summary-value\">\n          {{ bookingInfos.total }}\n        </div>\n      </div>\n      <div class=\"summary\">\n        <div class=\"icon-box\">\n          <i class=\"bi bi-bag-check checkin-icon\"></i>\n        </div>\n        <div class=\"summary-title\">\n          This month checkin\n        </div>\n        <div class=\"summary-value\">\n          {{ bookingInfos.lastMonth }}\n        </div>\n      </div>\n      <div class=\"summary\">\n        <div class=\"icon-box\">\n          <i class=\"bi bi-safe2 safe-icon\"></i>\n        </div>\n        <div class=\"summary-title\">\n          This month interest\n        </div>\n        <div class=\"summary-value\">\n          {{ bookingInfos.lastMonthTotal }}\n        </div>\n      </div>\n    </div>\n    <div>\n      <h2>Total booking of each month this year</h2>\n      <div>\n        <LineChart v-if=\"loaded\" :chartData=\"LineChartData\" :chartOptions=\"LineChartOptions\" />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { defineComponent, ref } from 'vue';\nimport axios from '../config'\nimport LineChart from '../components/LineChart.vue'\n\nexport default defineComponent({\n  name: 'DashboardView',\n  components: {\n    LineChart\n  },\n  data() {\n    return {\n      loaded: false,\n      LineChartData: {\n        labels: ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'],\n        datasets: [\n          {\n            data: [],\n          },\n        ],\n      },\n      LineChartOptions: {\n        plugins: {\n          legend: {\n            display: false\n          }\n        },\n        responsive: true,\n        maintainAspectRatio: false,\n        scales: {\n          y: {\n            min: 0,\n            ticks: {\n              stepSize: 1\n            }\n          },\n        },\n        elements: {\n          point: {\n            backgroundColor: 'red'\n          }\n        }\n      },\n      chartData: {\n        labels: ['January', 'February', 'March', 'April', 'May', 'June', 'July'],\n        datasets: [{\n          label: 'Income',\n          data: [10000, 15000, 20000, 18000, 17000, 25000, 30000],\n          backgroundColor: 'rgba(54, 162, 235, 0.2)',\n          borderColor: 'rgba(54, 162, 235, 1)',\n          borderWidth: 1\n        }]\n      },\n      chartOptions: {\n        scales: {\n          x: {\n            title: {\n              display: true,\n              text: 'Income'\n            },\n            ticks: {\n              beginAtZero: true\n            }\n          },\n          y: {\n            title: {\n              display: true,\n              text: 'Month'\n            }\n          }\n        }\n      }\n    }\n  },\n\n  setup() {\n    const guestInfos = ref([])\n    const bookingInfos = ref([])\n    const getData = async () => {\n      try {\n        let results = await axios.get('dashboard')\n        guestInfos.value = results.data.guestInfos\n        bookingInfos.value = results.data.bookingInfos\n      } catch (error) {\n        console.log(error)\n      }\n    }\n    getData()\n\n    return {\n      guestInfos,\n      bookingInfos,\n    }\n  },\n\n  async mounted() {\n    this.loaded = false\n\n    try {\n      const userlist = await axios.get('booking-this-year')\n      this.LineChartData.datasets[0].data = userlist.data\n      this.loaded = true\n    } catch (e) {\n      console.error(e)\n    }\n  }\n\n})\n</script>\n\n<style scoped> @media (min-width: 769px) {\n   .body {\n     background-color: rgb(240, 240, 240);\n     display: flex;\n     flex-direction: column;\n     padding: 20px;\n     border-radius: 3px;\n     height: 100%;\n   }\n\n   .table-box {\n     border: 2px rgb(222, 222, 222) solid;\n     border-radius: 15px;\n     padding: 5px;\n   }\n\n   tr:nth-child(even) {\n     background-color: rgb(224, 224, 224);\n   }\n\n   td:first-child