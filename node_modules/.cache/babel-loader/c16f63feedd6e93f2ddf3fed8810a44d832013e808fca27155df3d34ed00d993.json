{"ast":null,"code":"import { defineComponent, ref } from 'vue';\nimport axios from '../config';\nimport HotelChart from '../components/HotelChart.vue';\nexport default defineComponent({\n  name: 'DashboardView',\n  components: {\n    HotelChart\n  },\n  data() {\n    return {\n      loaded: false,\n      chartData: {\n        labels: ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'],\n        datasets: [{\n          label: 'Monthly Interest',\n          backgroundColor: 'rgba(0, 0, 0, 0)',\n          data: []\n        }]\n      },\n      chartOptions: {\n        //min scale to 1 and step is 1\n        //make the point to color red\n        plugins: {\n          legend: {\n            display: false\n          }\n        },\n        responsive: true,\n        maintainAspectRatio: false,\n        scales: {\n          y: {\n            min: 0,\n            ticks: {\n              stepSize: 1\n            }\n          }\n        },\n        elements: {\n          point: {\n            backgroundColor: 'red'\n          }\n        }\n      }\n    };\n  },\n  setup() {\n    const guestInfos = ref([]);\n    const bookingInfos = ref([]);\n    const getData = async () => {\n      try {\n        let results = await axios.get('dashboard');\n        guestInfos.value = results.data.guestInfos;\n        bookingInfos.value = results.data.bookingInfos;\n      } catch (error) {\n        console.log(error);\n      }\n    };\n    getData();\n    return {\n      guestInfos,\n      bookingInfos\n    };\n  },\n  async mounted() {\n    this.loaded = false;\n    try {\n      const userlist = await axios.get('booking-this-year');\n      this.chartData.datasets[0].data = userlist.data;\n      this.loaded = true;\n    } catch (e) {\n      console.error(e);\n    }\n  }\n});","map":{"version":3,"names":["defineComponent","ref","axios","HotelChart","name","components","data","loaded","chartData","labels","datasets","label","