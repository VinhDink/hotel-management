{"ast":null,"code":"import { defineComponent, ref, computed } from 'vue';\nimport axios from '../config';\nexport default defineComponent({\n  name: 'CheckinView',\n  data() {\n    return {\n      fee: {}\n    };\n  },\n  setup() {\n    const checkins = ref([]);\n    /**\n     * Get all checkins\n     * \n     * @return {void}\n     */\n    const getCheckin = async () => {\n      const response = await axios.get('checkin');\n      checkins.value = response.data;\n    };\n\n    /**\n     * Checkin a booking\n     * \n     * @param {number} id\n     * @param {number} bookingId\n     * \n     * @return {void}\n     */\n    const checkout = async (id, bookingId) => {\n      console.log(id, bookingId);\n      try {\n        const response = await axios.post('checked/checkout', {\n          id: id,\n          bookingId: bookingId\n        });\n        console.log(response.data);\n      } catch (error) {\n        console.error(error.response.data);\n      }\n      getCheckin();\n    };\n\n    /**\n     * Delete a checkin\n     * \n     * @param {number} id\n     * \n     * @return {void}\n     */\n    const destroy = async id => {\n      try {\n        const response = await axios.delete('checked/delCheck', {\n          data: {\n            id: id\n          }\n        });\n        console.log(response.data);\n      } catch (error) {\n        console.error(error.response.data);\n      }\n      getCheckin();\n    };\n\n    /**\n     * View detail of a checkin\n     * \n     * @param {number} employeeId\n     * @param {number} bookingId\n     * \n     * @return {void}\n     */\n    const employeeDetail = ref([]);\n    const bookingDetail = ref([]);\n    const viewDetail = async (employeeId, bookingId) => {\n      try {\n        const response = await axios.post('checkin/checkin-detail', {\n          employeeId: employeeId,\n          bookingId: bookingId\n        });\n        employeeDetail.value = response.data.employeeInfo;\n        bookingDetail.value = response.data.bookingInfo;\n        document.getElementById('detail-modal').style.display = 'block';\n      } catch (error) {\n        console.error(error.response.data);\n      }\n    };\n    const employeeInfo = computed(() => {\n      console.log(employeeDetail.value);\n      return employeeDetail.value;\n    });\n    const bookingInfo = computed(() => {\n      console.log(bookingDetail.value);\n      return bookingDetail.value;\n    });\n    const closeModal = () => {\n      document.getElementById('detail-modal').style.display = 'none';\n    };\n    getCheckin();\n    return {\n      checkins,\n      checkout,\n      destroy,\n      getCheckin,\n      viewDetail,\n      employeeInfo,\n      bookingInfo,\n      closeModal\n    };\n  },\n  methods: {\n    /**   \n    * Add fee to a checkin\n    * \n    * @param {number} id\n    * \n    * @return {void}\n    */\n\n    addFee(id) {\n      console.log(this.fee[id]);\n      try {\n        console.log(id, this.fee[id]);\n        const response = axios.post('checked/addFee', {\n          id: id,\n          fee: this.fee[id]\n        });\n        console.log(response.data);\n        this.getCheckin();\n      } catch (error) {\n        console.error(error.response.data);\n      }\n    }\n  }\n});","map":{"version":3,"names":["defineComponent","ref","computed","axios","name","data","fee","setup","checkins","getCheckin","response","get","value","checkout","id","bookingId","console","log","post","error","destroy","delete","employeeDetail","bookingDetail","viewDetail","employeeId","employeeInfo","bookingInfo","document","getElementById","style","display","closeModal","methods","addFee"],"sources":["/Users/vinh/Documents/Kiai/abc_hotel/src/views/CheckinView.vue"],"sourcesContent":["<template>\n  <div class=\"body\">\n    <div class=\"title-box\">\n      <h1 class=\"title\">Checkin</h1><br>\n    </div>\n    <div class=\"table-box\">\n      <table class=\"table\">\n        <tr class=\"\">\n          <th>Checkin ID</th>\n          <th>Booking ID</th>\n          <th>Employee ID</th>\n          <th>Checkin Time</th>\n          <th>Checkout Time</th>\n          <th>Fee</th>\n          <th>Total Price</th>\n        </tr>\n        <tr v-for=\"checkin in checkins\" :key=\"checkin.id\">\n          <td>{{ checkin.id }}</td>\n          <td>{{ checkin.booking_id }}</td>\n          <td>{{ checkin.employee_id }}</td>\n          <td>{{ checkin.checkin_time }}</td>\n          <td>{{ checkin.checkout_time }}</td>\n          <td>{{ checkin.fee }}</td>\n          <td>{{ checkin.total_price }}</td>\n          <td>\n            <div>\n              <input type=\"text\" v-model=\"fee[checkin.id]\" name=\"fee\" id=\"fee\" placeholder=\"Fee\" class=\"fee\">\n              <button type=\"submit\" class=\"submit-button\" @click=\"addFee(checkin.id)\">\n                <i class=\"bi bi-currency-dollar\"></i> Submit\n              </button>\n            </div>\n          </td>\n          <td>\n            <div>\n              <button @click=\"checkout(checkin.id, checkin.booking_id)\" class=\"modify-button\">\n                <i class=\"bi bi-box-arrow-right\"></i> Checkout\n              </button>\n            </div>\n          </td>\n          <td>\n            <div>\n              <button @click=\"viewDetail(checkin.employee_id, checkin.booking_id)\" class=\"modify-button yellow-color\">\n                <i class=\"bi bi-box-arrow-right\"></i> View detail\n              </button>\n            </div>\n          </td>\n          <td>\n            <div class=\"fee-box\">\n              <button @click=\"destroy(checkin.id)\" class=\"danger-button\">\n                <i class=\"bi bi-trash\"></i> Delete\n              </button>\n            </div>\n          </td>\n        </tr>\n      </table>\n    </div>\n    <div class=\"detail-modal\" id=\"detail-modal\">\n      <div class=\"modal-content\">\n        <div class=\"modal-header\">\n          <span>Detail</span>\n          <span class=\"close\" @click=\"closeModal()\">&times;</span>\n        </div>\n        <div class=\"modal-body\">\n          <div class=\"modal-icon\">\n            <i class=\"bi bi-info-circle-fill\"></i>\n          </div>\n          <div>\n            <div><span class=\"modal-title\">Employee id: </span>{{ employeeInfo.id }}</div>\n            <div><span class=\"modal-title\">Employee name: </span>{{ employeeInfo.name }}</div>\n            <div><span class=\"modal-title\">Employee role: </span>{{ employeeInfo.role }}</div>\n            <div><span class=\"modal-title\">Booking id: </span> {{ bookingInfo.id }}</div>\n            <div><span class=\"modal-title\">Guest name: </span>{{ bookingInfo.guest_name }}</div>\n            <div><span class=\"modal-title\">Guest number: </span>{{ bookingInfo.guest_number }}</div>\n            <div><span class=\"modal-title\">Booked time: </span> {{ bookingInfo.created_at }}</div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { defineComponent, ref, computed } from 'vue'\nimport axios from '../config'\n\nexport default defineComponent({\n  name: 'CheckinView',\n  data() {\n    return {\n      fee: {}\n    }\n  },\n\n  setup() {\n\n    const checkins = ref([]);\n    /**\n     * Get all checkins\n     * \n     * @return {void}\n     */\n    const getCheckin = async () => {\n      const response = await axios.get('checkin')\n      checkins.value = response.data\n    }\n\n    /**\n     * Checkin a booking\n     * \n     * @param {number} id\n     * @param {number} bookingId\n     * \n     * @return {void}\n     */\n    const checkout = async (id, bookingId) => {\n      console.log(id, bookingId)\n      try {\n        const response = await axios.post('checked/checkout', {\n          id: id,\n          bookingId: bookingId\n        })\n        console.log(response.data);\n      } catch (error) {\n        console.error(error.response.data)\n      }\n      getCheckin()\n    }\n\n    /**\n     * Delete a checkin\n     * \n     * @param {number} id\n     * \n     * @return {void}\n     */\n    const destroy = async (id) => {\n      try {\n        const response = await axios.delete('checked/delCheck', {\n          data: {\n            id: id\n          }\n        })\n        console.log(response.data);\n      } catch (error) {\n        console.error(error.response.data)\n      }\n      getCheckin()\n    }\n\n    /**\n     * View detail of a checkin\n     * \n     * @param {number} employeeId\n     * @param {number} bookingId\n     * \n     * @return {void}\n     */\n    const employeeDetail = ref([]);\n    const bookingDetail = ref([]);\n    const viewDetail = async (employeeId, bookingId) => {\n      try {\n        const response = await axios.post('checkin/checkin-detail', {\n          employeeId: employeeId,\n          bookingId: bookingId\n        })\n        employeeDetail.value = response.data.employeeInfo\n        bookingDetail.value = response.data.bookingInfo\n        document.getElementById('detail-modal').style.display = 'block'\n      } catch (error) {\n        console.error(error.response.data)\n      }\n    }\n\n    const employeeInfo = computed(() => {\n      console.log(employeeDetail.value)\n      return employeeDetail.value\n    })\n\n    const bookingInfo = computed(() => {\n      console.log(bookingDetail.value)\n      return bookingDetail.value\n    })\n\n    const closeModal = () => {\n      document.getElementById('detail-modal').style.display = 'none'\n    }\n\n    getCheckin()\n\n    return {\n      checkins,\n      checkout,\n      destroy,\n      getCheckin,\n      viewDetail,\n      employeeInfo,\n      bookingInfo,\n      closeModal\n    }\n  },\n\n  methods: {\n    /**   \n   * Add fee to a checkin\n   * \n   * @param {number} id\n   * \n   * @return {void}\n   */\n\n    addFee(id) {\n      console.log(this.fee[id])\n      try {\n        console.log(id, this.fee[id]);\n        const response = axios.post('checked/addFee', {\n          id: id,\n          fee: this.fee[id]\n        })\n        console.log(response.data);\n        this.getCheckin()\n      } catch (error) {\n        console.error(error.response.data)\n      }\n    }\n  },\n})\n</script>\n\n<style scoped>\n.modal-title {\n  font-weight: bold;\n}\n\n.yellow-color {\n  background-color: #f5d442;\n}\n\n.modal-body {\n  padding: 15px;\n  display: flex;\n}\n\n.modal-icon {\n  font-size: 80px;\n  padding: 20px;\n  color: #f5d442;\n}\n\n.modal-header {\n  display: flex;\n  color: white;\n  font-size: 20px;\n  width: 100%;\n  padding: 15px;\n  background-color: #f5d442;\n}\n\n.detail-modal {\n  display: none;\n  position: fixed;\n  /* Stay in place */\n  z-index: 1;\n  /* Sit on top */\n  left: 0;\n  top: 0;\n  width: 100%;\n  /* Full width */\n  height: 100%;\n  background-color: rgb(0, 0, 0);\n  /* Fallback color */\n  background-color: rgba(0, 0, 0, 0.4);\n  /* Black w/ opacity */\n}\n\n.modal-content {\n  background-color: #fefefe;\n  margin: 15% auto;\n  /* 15% from the top and centered */\n  border: 1px solid #888;\n  max-width: 800px;\n  height: 300px;\n  /* Could be more or less, depending on screen size */\n}\n\n.close {\n  color: #ffffff;\n  font-size: 20px;\n  display: flex;\n  flex-direction: column-reverse;\n  font-weight: bold;\n  height: 100%\n}\n\n.close:hover,\n.close:focus {\n  color: black;\n  text-decoration: none;\n  cursor: pointer;\n}\n\n@media (min-width: 426px) {\n\n  .fee {\n    padding: 5px;\n  }\n\n  .body {\n    background-color: rgb(240, 240, 240);\n    display: flex;\n    flex-direction: column;\n    padding: 20px;\n    border-radius: 3px;\n    height: 100%;\n  }\n\n  .title-box {\n    display: flex;\n    justify-content: space-between;\n    margin-bottom: 40px;\n  }\n\n  .title {\n    order: 0;\n    font-weight: bold;\n  }\n}\n\n@media (max-width:768px) {\n\n  .body {\n    background-color: rgb(240, 240, 240);\n    display: flex;\n    flex-direction: column;\n    min-height: 100vh;\n  }\n\n  .submit-button {\n    border: none;\n    background: #93e4c1;\n    color: white;\n    border-top-right-radius: 8px;\n    border-bottom-right-radius: 8px;\n  }\n\n  .fee {\n    width: 100px;\n    border: none;\n    border-top-left-radius: 8px;\n    border-bottom-left-radius: 8px;\n    padding: 5px;\n  }\n\n  .title-box {\n    padding: 10px;\n    font-weight: bold;\n  }\n\n  .title {\n    order: 0;\n    font-weight: bold;\n  }\n}\n</style>\n"],"mappings":"AAkFA,SAASA,eAAe,EAAEC,GAAG,EAAEC,QAAO,QAAS,KAAI;AACnD,OAAOC,KAAI,MAAO,WAAU;AAE5B,eAAeH,eAAe,CAAC;EAC7BI,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE,CAAC;IACR;EACF,CAAC;EAEDC,KAAKA,CAAA,EAAG;IAEN,MAAMC,QAAO,GAAIP,GAAG,CAAC,EAAE,CAAC;IACxB;;;;;IAKA,MAAMQ,UAAS,GAAI,MAAAA,CAAA,KAAY;MAC7B,MAAMC,QAAO,GAAI,MAAMP,KAAK,CAACQ,GAAG,CAAC,SAAS;MAC1CH,QAAQ,CAACI,KAAI,GAAIF,QAAQ,CAACL,IAAG;IAC/B;;IAEA;;;;;;;;IAQA,MAAMQ,QAAO,GAAI,MAAAA,CAAOC,EAAE,EAAEC,SAAS,KAAK;MACxCC,OAAO,CAACC,GAAG,CAACH,EAAE,EAAEC,SAAS;MACzB,IAAI;QACF,MAAML,QAAO,GAAI,MAAMP,KAAK,CAACe,IAAI,CAAC,kBAAkB,EAAE;UACpDJ,EAAE,EAAEA,EAAE;UACNC,SAAS,EAAEA;QACb,CAAC;QACDC,OAAO,CAACC,GAAG,CAACP,QAAQ,CAACL,IAAI,CAAC;MAC5B,EAAE,OAAOc,KAAK,EAAE;QACdH,OAAO,CAACG,KAAK,CAACA,KAAK,CAACT,QAAQ,CAACL,IAAI;MACnC;MACAI,UAAU,CAAC;IACb;;IAEA;;;;;;;IAOA,MAAMW,OAAM,GAAI,MAAON,EAAE,IAAK;MAC5B,IAAI;QACF,MAAMJ,QAAO,GAAI,MAAMP,KAAK,CAACkB,MAAM,CAAC,kBAAkB,EAAE;UACtDhB,IAAI,EAAE;YACJS,EAAE,EAAEA;UACN;QACF,CAAC;QACDE,OAAO,CAACC,GAAG,CAACP,QAAQ,CAACL,IAAI,CAAC;MAC5B,EAAE,OAAOc,KAAK,EAAE;QACdH,OAAO,CAACG,KAAK,CAACA,KAAK,CAACT,QAAQ,CAACL,IAAI;MACnC;MACAI,UAAU,CAAC;IACb;;IAEA;;;;;;;;IAQA,MAAMa,cAAa,GAAIrB,GAAG,CAAC,EAAE,CAAC;IAC9B,MAAMsB,aAAY,GAAItB,GAAG,CAAC,EAAE,CAAC;IAC7B,MAAMuB,UAAS,GAAI,MAAAA,CAAOC,UAAU,EAAEV,SAAS,KAAK;MAClD,IAAI;QACF,MAAML,QAAO,GAAI,MAAMP,KAAK,CAACe,IAAI,CAAC,wBAAwB,EAAE;UAC1DO,UAAU,EAAEA,UAAU;UACtBV,SAAS,EAAEA;QACb,CAAC;QACDO,cAAc,CAACV,KAAI,GAAIF,QAAQ,CAACL,IAAI,CAACqB,YAAW;QAChDH,aAAa,CAACX,KAAI,GAAIF,QAAQ,CAACL,IAAI,CAACsB,WAAU;QAC9CC,QAAQ,CAACC,cAAc,CAAC,cAAc,CAAC,CAACC,KAAK,CAACC,OAAM,GAAI,OAAM;MAChE,EAAE,OAAO