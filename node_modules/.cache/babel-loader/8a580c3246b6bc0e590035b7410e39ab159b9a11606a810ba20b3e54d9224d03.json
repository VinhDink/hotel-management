{"ast":null,"code":"import { defineComponent, ref } from 'vue';\nimport axios from '../config';\nexport default defineComponent({\n  name: 'UserListView',\n  setup() {\n    const users = ref([]);\n\n    /**\n     * Get all users\n     * \n     * @return {void}\n     */\n    const getUser = async () => {\n      const response = await axios.get('userlist');\n      users.value = response.data;\n    };\n\n    /**\n     * Delete a user\n     * \n     * @param {int} id \n     * \n     * @return {void}\n     */\n    const deleteUser = async id => {\n      console.log(id);\n      try {\n        const response = await axios.delete('userlist/del', {\n          data: {\n            id: id\n          }\n        });\n        console.log(response.data);\n        getUser();\n      } catch (error) {\n        console.error(error.response.data);\n      }\n    };\n    getUser();\n    return {\n      users,\n      deleteUser\n    };\n  }\n});","map":{"version":3,"names":["defineComponent","ref","axios","name","setup","users","getUser","response","get","value","data","deleteUser","id","console","log","delete","error"],"sources":["/Users/vinh/Documents/Kiai/abc_hotel/src/views/UserListView.vue"],"sourcesContent":["<template>\n  <div class=\"body\">\n    <div class=\"title-box\">\n      <h1 class=title>List of user:</h1>\n    </div>\n    <div>\n      <div class=\"table-box\">\n        <table class=\"table\">\n          <tr>\n            <th>ID</th>\n            <th>Username</th>\n            <th>Role</th>\n            <th>Actions</th>\n          </tr>\n          <tr v-for=\"user in users\" :key=\"user.id\">\n            <td>{{ user.id }}</td>\n            <td>{{ user.username }}</td>\n            <td>{{ user.role }}</td>\n            <td>\n              <button @click=\"deleteUser(user.id)\" class=\"delete-button\">\n                <i class=\"bi bi-trash\"></i> Delete\n              </button>\n            </td>\n          </tr>\n        </table>\n      </div>\n    </div>\n    <!-- <table class=\"table table-striped\">\n      <thead>\n        <tr>\n          <th>ID</th>\n          <th>Username</th>\n          <th>Role</th>\n          <th>Actions</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"user in users\" :key=\"user.id\">\n          <td>{{ user.id }}</td>\n          <td>{{ user.username }}</td>\n          <td>{{ user.role }}</td>\n          <td>\n            <button @click=\"deleteUser(user.id)\" class=\"btn btn-danger\">\n              <i class=\"bi bi-trash\"></i> Delete\n            </button>\n          </td>\n        </tr>\n      </tbody>\n    </table> -->\n  </div>\n</template>\n\n<script>\nimport { defineComponent, ref } from 'vue'\nimport axios from '../config'\n\nexport default defineComponent({\n  name: 'UserListView',\n\n  setup() {\n    const users = ref([]);\n\n    /**\n     * Get all users\n     * \n     * @return {void}\n     */\n    const getUser = async () => {\n      const response = await axios.get('userlist')\n      users.value = response.data\n    }\n\n    /**\n     * Delete a user\n     * \n     * @param {int} id \n     * \n     * @return {void}\n     */\n    const deleteUser = async (id) => {\n      console.log(id)\n      try {\n        const response = await axios.delete('userlist/del', {\n          data: {\n            id: id,\n          },\n        })\n        console.log(response.data)\n        getUser()\n      } catch (error) {\n        console.error(error.response.data)\n      }\n    }\n    getUser()\n\n    return {\n      users,\n      deleteUser\n    }\n  },\n})\n</script>\n<style scoped>\n@media (min-width: 769px) {\n  .body {\n    background-color: rgb(240, 240, 240);\n    display: flex;\n    flex-direction: column;\n    padding: 20px;\n    border-radius: 3px;\n    height: 100%;\n  }\n\n  .table-box {\n    border: 2px rgb(222, 222, 222) solid;\n    border-radius: 13px;\n    padding: 5px;\n  }\n\n  th {\n    padding: 10px\n  }\n\n  .delete-button {\n    border: none;\n    border-radius: 20px