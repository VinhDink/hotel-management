{"ast":null,"code":"import DataTable from '../components/DataTable.vue';\nimport { ref } from 'vue';\nimport axios from '../config';\nimport DetailInfo from '../components/DetailInfo.vue';\nexport default {\n  name: 'TestingView',\n  components: {\n    DataTable,\n    DetailInfo\n  },\n  setup() {\n    const tableData = ref({});\n    const roomInfo = ref({});\n    const modalStatus = ref(false);\n    const tableTitles = ref(['Room ID', 'Room Name', 'Type', 'Actions']);\n    const getRooms = async () => {\n      let results = await axios.get('room');\n      tableData.value = results.data;\n    };\n    const viewRoomDetail = async id => {\n      let response = await axios.post('room-detail', {\n        id: id\n      });\n      roomInfo.value = {\n        'Id': response.data.id,\n        'Name': response.data.name,\n        'Type': response.data.type,\n        'Hour price': response.data.hour_price,\n        'Day price': response.data.day_price,\n        'Status': response.data.status,\n        'Size': response.data.size,\n        'Balcony': response.data.balcony,\n        'View': response.data.view,\n        'Smoking': response.data.smoking,\n        'Floor': response.data.floor,\n        'Bathtub': response.data.bathtub\n      };\n      console.log(response.data.id);\n      document.getElementById('detail-modal').style.display = 'block';\n    };\n    getRooms();\n    return {\n      tableData,\n      tableTitles,\n      viewRoomDetail,\n      roomInfo,\n      modalStatus\n    };\n  }\n};","map":{"version":3,"names":["DataTable","ref","axios","DetailInfo","name","components","setup","tableData","roomInfo","modalStatus","tableTitles","getRooms","results","get","value","data","viewRoomDetail","id","response","post","type","hour_price","day_price","status","size","balcony","view","smoking","floor","bathtub","console","log","document","getElementById","style","display"],"sources":["/Users/vinh/Documents/Kiai/abc_hotel/src/views/RoomView.vue"],"sourcesContent":["<template>\n  <div class=\"body\">\n    <div class=\"title-box\">\n      <h1 class=\"title\">Room</h1>\n    </div>\n    <div>\n      <DataTable :tableDatas=\"tableData\" :tableTitles=\"tableTitles\" :actionModify=\"'Modify'\" :actionDetail=\"'View detail'\"\n        :functionFirst=\"viewRoomDetail\"></DataTable>\n      <div>\n        <DetailInfo :datas=\"roomInfo\"></DetailInfo>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport DataTable from '../components/DataTable.vue'\nimport { ref } from 'vue'\nimport axios from '../config'\nimport DetailInfo from '../components/DetailInfo.vue'\n\nexport default {\n  name: 'TestingView',\n  components: {\n    DataTable,\n    DetailInfo\n  },\n\n  setup() {\n    const tableData = ref({})\n    const roomInfo = ref({})\n    const modalStatus = ref(false)\n    const tableTitles = ref([\n      'Room ID',\n      'Room Name',\n      'Type',\n      'Actions',\n    ])\n    const getRooms = async () => {\n      let results = await axios.get('room')\n      tableData.value = results.data\n    }\n\n    const viewRoomDetail = async (id) => {\n      let response = await axios.post('room-detail', {\n        id: id\n      })\n      roomInfo.value = {\n        'Id': response.data.id,\n        'Name': response.data.name,\n        'Type': response.data.type,\n        'Hour price': response.data.hour_price,\n        'Day price': response.data.day_price,\n        'Status': response.data.status,\n        'Size': response.data.size,\n        'Balcony': response.data.balcony,\n        'View': response.data.view,\n        'Smoking': response.data.smoking,\n        'Floor': response.data.floor,\n        'Bathtub': response.data.bathtub,\n      }\n      console.log(response.data.id)\n\n      document.getElementById('detail-modal').style.display = 'block'\n    }\n\n    getRooms()\n    return {\n      tableData,\n      tableTitles,\n      viewRoomDetail,\n      roomInfo,\n      modalStatus,\n    }\n  }\n}\n</script>\n\n\n<style scoped>\n@media (min-width: 769px) {\n  .body {\n    background-color: rgb(240, 240, 240);\n    display: flex;\n    flex-direction: column;\n    padding: 20px;\n    border-radius: 3px;\n    min-height: 100%;\n  }\n\n}\n\n@media (max-width:768px) {\n  .body {\n    background-color: rgb(240, 240, 240);\n    display: flex;\n    flex-direction: column;\n    min-height: 100vh;\n  }\n}\n</style>\n"],"mappings":"AAgBA,OAAOA,SAAQ,MAAO,6BAA4B;AAClD,SAASC,GAAE,QAAS,KAAI;AACxB,OAAOC,KAAI,MAAO,WAAU;AAC5B,OAAOC,UAAS,MAAO,8BAA6B;AAEpD,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACVL,SAAS;IACTG;EACF,CAAC;EAEDG,KAAKA,CAAA,EAAG;IACN,MAAMC,SAAQ,GAAIN,GAAG,CAAC,CAAC,CAAC;IACxB,MAAMO,QAAO,GAAIP,GAAG,CAAC,CAAC,CAAC;IACvB,MAAMQ,WAAU,GAAIR,GAAG,CAAC,KAAK;IAC7B,MAAMS,WAAU,GAAIT,GAAG,CAAC,CACtB,SAAS,EACT,WAAW,EACX,MAAM,EACN,SAAS,CACV;IACD,MAAMU,QAAO,GAAI,MAAAA,CAAA,KAAY;MAC3B,IAAIC,OAAM,GAAI,MAAMV,KAAK,CAACW,GAAG,CAAC,MAAM;MACpCN,SAAS,CAACO,KAAI,GAAIF,OAAO,CAACG,IAAG;IAC/B;IAEA,MAAMC,cAAa,GAAI,MAAOC,EAAE,IAAK;MACnC,IAAIC,QAAO,GAAI,MAAMhB,KAAK,CAACiB,IAAI,CAAC,aAAa,EAAE;QAC7CF,EAAE,EAAEA;MACN,CAAC;MACDT,QAAQ,CAACM,KAAI,GAAI;QACf,IAAI,EAAEI,QAAQ,CAACH,IAAI,CAACE,EAAE;QACtB,MAAM,EAAEC,QAAQ,CAACH,IAAI,CAACX,IAAI;QAC1B,MAAM,EAAEc,QAAQ,CAACH,IAAI,CAACK,IAAI;QAC1B,YAAY,EAAEF,QAAQ,CAACH,IAAI,CAACM,UAAU;QACtC,WAAW,EAAEH,QAAQ,CAACH,IAAI,CAACO,SAAS;QACpC,QAAQ,EAAEJ,QAAQ,CAACH,IAAI,CAACQ,MAAM;QAC9B,MAAM,EAAEL,QAAQ,CAACH,IAAI,CAACS,IAAI;QAC1B,SAAS,EAAEN,QAAQ,CAACH,IAAI,CAACU,OAAO;QAChC,MAAM,EAAEP,QAAQ,CAACH,IAAI,CAACW,IAAI;QAC1B,SAAS,EAAER,QAAQ,CAACH,IAAI,CAACY,OAAO;QAChC,OAAO,EAAET,QAAQ,CAACH,IAAI,CAACa,KAAK;QAC5B,SAAS,EAAEV,QAAQ,CAACH,IAAI,CAACc;MAC3B;MACAC,OAAO,CAACC,GAAG,CAACb,QAAQ,CAACH,IAAI,CAACE,EAAE;MAE5Be,QAAQ,CAACC,cAAc,CAAC,cAAc,CAAC,CAACC,KAAK,CAACC,OAAM,GAAI,OAAM;IAChE;IAEAxB,QAAQ,CAAC;IACT,OAAO;MACLJ,SAAS;MACTG,WAAW;MACXM,cAAc;MACdR,QAAQ;MACRC;IACF;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}