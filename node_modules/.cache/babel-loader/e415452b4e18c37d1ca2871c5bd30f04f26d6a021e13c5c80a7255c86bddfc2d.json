{"ast":null,"code":"import DataTable from '../components/DataTable.vue';\nimport { ref } from 'vue';\nimport axios from '../config';\nexport default {\n  name: 'TestingView',\n  components: {\n    DataTable\n  },\n  setup() {\n    const tableData = ref({});\n    const roomInfo = ref({});\n    const tableTitles = ref(['Room ID', 'Room Name', 'Type', 'Actions']);\n    const getRooms = async () => {\n      let results = await axios.get('room');\n      tableData.value = results.data;\n      console.log(tableData.value);\n    };\n    const viewRoomDetail = id => {\n      for (let i = 0; i < tableData.value.length; i++) {\n        if (tableData.value[i].id == id) {\n          roomInfo.value = tableData.value[i];\n          document.getElementById('detail-modal').style.display = 'block';\n        }\n      }\n    };\n    getRooms();\n    return {\n      tableData,\n      tableTitles,\n      viewRoomDetail\n    };\n  }\n};","map":{"version":3,"names":["DataTable","ref","axios","name","components","setup","tableData","roomInfo","tableTitles","getRooms","results","get","value","data","console","log","viewRoomDetail","id","i","length","document","getElementById","style","display"],"sources":["/Users/vinh/Documents/Kiai/abc_hotel/src/views/TestingView.vue"],"sourcesContent":["<template>\n    <div>\n        <DataTable :tableDatas=\"tableData\" :tableTitles=\"tableTitles\" :actionModify=\"true\" :actionDetail=\"true\"></DataTable>\n        <div class=\"detail-modal\" id=\"detail-modal\">\n            <div class=\"modal-content\">\n                <div class=\"modal-header\">\n                    <span>Detail</span>\n                    <span class=\"close\" @click=\"closeModal()\">&times;</span>\n                </div>\n                <div class=\"modal-body\">\n                    <div class=\"modal-icon\">\n                        <i class=\"bi bi-info-circle-fill\"></i>\n                    </div>\n                    <div>\n                        <div><span class=\"modal-title\">Room id: </span>{{ roomInfo.id }}</div>\n                        <div><span class=\"modal-title\">Room name: </span>{{ roomInfo.name }}</div>\n                        <div><span class=\"modal-title\">Room type: </span>{{ roomInfo.type }}</div>\n                        <div><span class=\"modal-title\">Hour price: </span> {{ roomInfo.hour_price }}</div>\n                        <div><span class=\"modal-title\">Day price: </span>{{ roomInfo.day_price }}</div>\n                        <div><span class=\"modal-title\">Status: </span><span v-if=\"roomInfo.status == 1\">Available</span>\n                            <span v-if=\"roomInfo.status == 0\">Unavailable</span>\n                        </div>\n                        <div><span class=\"modal-title\">Size: </span> {{ roomInfo.size }}</div>\n                        <div><span class=\"modal-title\">Balcony: </span><span v-if=\"roomInfo.balcony == 1\">Yes</span>\n                            <span v-if=\"roomInfo.balcony == 0\">No</span>\n                        </div>\n                        <div><span class=\"modal-title\">View: </span> {{ roomInfo.view }}</div>\n                        <div><span class=\"modal-title\">Smoking: </span><span v-if=\"roomInfo.smoking == 1\">Yes</span>\n                            <span v-if=\"roomInfo.smoking == 0\">No</span>\n                        </div>\n                        <div><span class=\"modal-title\">Floor: </span> {{ roomInfo.floor }}</div>\n                        <div><span class=\"modal-title\">Bathtub: </span><span v-if=\"roomInfo.bathtub == 1\">Yes</span>\n                            <span v-if=\"roomInfo.bathtub == 0\">No</span>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport DataTable from '../components/DataTable.vue'\nimport { ref } from 'vue'\nimport axios from '../config'\n\nexport default {\n    name: 'TestingView',\n    components: {\n        DataTable\n    },\n\n    setup() {\n        const tableData = ref({})\n        const roomInfo = ref({})\n        const tableTitles = ref([\n            'Room ID',\n            'Room Name',\n            'Type',\n            'Actions',\n        ])\n        const getRooms = async () => {\n            let results = await axios.get('room')\n            tableData.value = results.data\n            console.log(tableData.value)\n        }\n\n        const viewRoomDetail = (id) => {\n            for (let i = 0; i < tableData.value.length; i++) {\n                if (tableData.value[i].id == id) {\n                    roomInfo.value = tableData.value[i]\n                    document.getElementById('detail-modal').style.display = 'block'\n                }\n            }\n        }\n\n        getRooms()\n        return {\n            tableData,\n            tableTitles,\n            viewRoomDetail,\n        }\n    }\n}\n</script>\n"],"mappings":"AA0CA,OAAOA,SAAQ,MAAO,6BAA4B;AAClD,SAASC,GAAE,QAAS,KAAI;AACxB,OAAOC,KAAI,MAAO,WAAU;AAE5B,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACRJ;EACJ,CAAC;EAEDK,KAAKA,CAAA,EAAG;IACJ,MAAMC,SAAQ,GAAIL,GAAG,CAAC,CAAC,CAAC;IACxB,MAAMM,QAAO,GAAIN,GAAG,CAAC,CAAC,CAAC;IACvB,MAAMO,WAAU,GAAIP,GAAG,CAAC,CACpB,SAAS,EACT,WAAW,EACX,MAAM,EACN,SAAS,CACZ;IACD,MAAMQ,QAAO,GAAI,MAAAA,CAAA,KAAY;MACzB,IAAIC,OAAM,GAAI,MAAMR,KAAK,CAACS,GAAG,CAAC,MAAM;MACpCL,SAAS,CAACM,KAAI,GAAIF,OAAO,CAACG,IAAG;MAC7BC,OAAO,CAACC,GAAG,CAACT,SAAS,CAACM,KAAK;IAC/B;IAEA,MAAMI,cAAa,GAAKC,EAAE,IAAK;MAC3B,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAIZ,SAAS,CAACM,KAAK,CAACO,MAAM,EAAED,CAAC,EAAE,EAAE;QAC7C,IAAIZ,SAAS,CAACM,KAAK,CAACM,CAAC,CAAC,CAACD,EAAC,IAAKA,EAAE,EAAE;UAC7BV,QAAQ,CAACK,KAAI,GAAIN,SAAS,CAACM,KAAK,CAACM,CAAC;UAClCE,QAAQ,CAACC,cAAc,CAAC,cAAc,CAAC,CAACC,KAAK,CAACC,OAAM,GAAI,OAAM;QAClE;MACJ;IACJ;IAEAd,QAAQ,CAAC;IACT,OAAO;MACHH,SAAS;MACTE,WAAW;MACXQ;IACJ;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}