{"ast":null,"code":"import { defineComponent, ref, computed } from 'vue';\nimport axios from '../config';\nexport default defineComponent({\n  name: 'CheckinView',\n  data() {\n    return {\n      fee: {}\n    };\n  },\n  setup() {\n    const checkins = ref([]);\n    /**\n     * Get all checkins\n     * \n     * @return {void}\n     */\n    const getCheckin = async () => {\n      const response = await axios.get('checkin');\n      checkins.value = response.data;\n    };\n\n    /**\n     * Checkin a booking\n     * \n     * @param {number} id\n     * @param {number} bookingId\n     * \n     * @return {void}\n     */\n    const checkout = async (id, bookingId) => {\n      console.log(id, bookingId);\n      try {\n        const response = await axios.post('checked/checkout', {\n          id: id,\n          bookingId: bookingId\n        });\n        console.log(response.data);\n      } catch (error) {\n        console.error(error.response.data);\n      }\n      getCheckin();\n    };\n\n    /**\n     * Delete a checkin\n     * \n     * @param {number} id\n     * \n     * @return {void}\n     */\n    const destroy = async id => {\n      try {\n        const response = await axios.delete('checked/delCheck', {\n          data: {\n            id: id\n          }\n        });\n        console.log(response.data);\n      } catch (error) {\n        console.error(error.response.data);\n      }\n      getCheckin();\n    };\n\n    /**\n     * View detail of a checkin\n     * \n     * @param {number} employeeId\n     * @param {number} bookingId\n     * \n     * @return {void}\n     */\n    const employeeDetail = ref([]);\n    const bookingDetail = ref([]);\n    const viewDetail = async (employeeId, bookingId) => {\n      try {\n        const response = await axios.post('checkin/checkin-detail', {\n          employeeId: employeeId,\n          bookingId: bookingId\n        });\n        console.log(response.data);\n        employeeDetail.value = response.data.employeeDetail;\n        bookingDetail.value = response.data.bookingDetail;\n      } catch (error) {\n        console.error(error.response.data);\n      }\n    };\n    const employeeInfo = computed(() => {\n      return employeeDetail.value;\n    });\n    const bookingInfo = computed(() => {\n      return bookingDetail.value;\n    });\n\n    /**   \n     * Add fee to a checkin\n     * \n     * @param {number} id\n     * \n     * @return {void}\n     */\n\n    getCheckin();\n    return {\n      checkins,\n      checkout,\n      destroy,\n      getCheckin,\n      viewDetail,\n      employeeInfo,\n      bookingInfo\n    };\n  },\n  methods: {\n    addFee(id) {\n      console.log(this.fee[id]);\n      try {\n        console.log(id, this.fee[id]);\n        const response = axios.post('checked/addFee', {\n          id: id,\n          fee: this.fee[id]\n        });\n        console.log(response.data);\n        this.getCheckin();\n      } catch (error) {\n        console.error(error.response.data);\n      }\n    }\n  }\n});","map":{"version":3,"names":["defineComponent","ref","computed","axios","name","data","fee","setup","checkins","getCheckin","response","get","value","checkout","id","bookingId","console","log","post","error","destroy","delete","employeeDetail","bookingDetail","viewDetail","employeeId","employeeInfo","bookingInfo","methods","addFee"],"sources":["/Users/vinh/Documents/Kiai/abc_hotel/src/views/CheckinView.vue"],"sourcesContent":["<template>\n  <div class=\"body\">\n    <div class=\"title-box\">\n      <h1 class=\"title\">Checkin</h1><br>\n    </div>\n    <div class=\"table-box\">\n      <table class=\"table\">\n        <tr class=\"\">\n          <th>Checkin ID</th>\n          <th>Booking ID</th>\n          <th>Employee ID</th>\n          <th>Checkin Time</th>\n          <th>Checkout Time</th>\n          <th>Fee</th>\n          <th>Total Price</th>\n        </tr>\n        <tr v-for=\"checkin in checkins\" :key=\"checkin.id\">\n          <td>{{ checkin.id }}</td>\n          <td>{{ checkin.booking_id }}</td>\n          <td>{{ checkin.employee_id }}</td>\n          <td>{{ checkin.checkin_time }}</td>\n          <td>{{ checkin.checkout_time }}</td>\n          <td>{{ checkin.fee }}</td>\n          <td>{{ checkin.total_price }}</td>\n          <td>\n            <div>\n              <input type=\"text\" v-model=\"fee[checkin.id]\" name=\"fee\" id=\"fee\" placeholder=\"Fee\" class=\"fee\">\n              <button type=\"submit\" class=\"submit-button\" @click=\"addFee(checkin.id)\">\n                <i class=\"bi bi-currency-dollar\"></i> Submit\n              </button>\n            </div>\n          </td>\n          <td>\n            <div>\n              <button @click=\"checkout(checkin.id, checkin.booking_id)\" class=\"modify-button\">\n                <i class=\"bi bi-box-arrow-right\"></i> Checkout\n              </button>\n            </div>\n          </td>\n          <td>\n            <div class=\"fee-box\">\n              <button @click=\"destroy(checkin.id)\" class=\"danger-button\">\n                <i class=\"bi bi-trash\"></i> Delete\n              </button>\n            </div>\n          </td>\n          <td>\n            <div>\n              <button @click=\"viewDetail(checkin.employee_id, checkin.booking_id)\" class=\"modify-button\">\n                <i class=\"bi bi-box-arrow-right\"></i> View detail\n              </button>\n            </div>\n          </td>\n        </tr>\n      </table>\n    </div>\n    <div class=\"modal\">\n      {{  }}\n      {{ bookingDetail }}\n    </div>\n  </div>\n</template>\n\n<script>\nimport { defineComponent, ref, computed } from 'vue'\nimport axios from '../config'\n\nexport default defineComponent({\n  name: 'CheckinView',\n  data() {\n    return {\n      fee: {}\n    }\n  },\n\n  setup() {\n\n    const checkins = ref([]);\n    /**\n     * Get all checkins\n     * \n     * @return {void}\n     */\n    const getCheckin = async () => {\n      const response = await axios.get('checkin')\n      checkins.value = response.data\n    }\n\n    /**\n     * Checkin a booking\n     * \n     * @param {number} id\n     * @param {number} bookingId\n     * \n     * @return {void}\n     */\n    const checkout = async (id, bookingId) => {\n      console.log(id, bookingId)\n      try {\n        const response = await axios.post('checked/checkout', {\n          id: id,\n          bookingId: bookingId\n        })\n        console.log(response.data);\n      } catch (error) {\n        console.error(error.response.data)\n      }\n      getCheckin()\n    }\n\n    /**\n     * Delete a checkin\n     * \n     * @param {number} id\n     * \n     * @return {void}\n     */\n    const destroy = async (id) => {\n      try {\n        const response = await axios.delete('checked/delCheck', {\n          data: {\n            id: id\n          }\n        })\n        console.log(response.data);\n      } catch (error) {\n        console.error(error.response.data)\n      }\n      getCheckin()\n    }\n\n    /**\n     * View detail of a checkin\n     * \n     * @param {number} employeeId\n     * @param {number} bookingId\n     * \n     * @return {void}\n     */\n    const employeeDetail = ref([]);\n    const bookingDetail = ref([]);\n    const viewDetail = async (employeeId, bookingId) => {\n      try {\n        const response = await axios.post('checkin/checkin-detail', {\n          employeeId: employeeId,\n          bookingId: bookingId\n        })\n        console.log(response.data);\n        employeeDetail.value = response.data.employeeDetail\n        bookingDetail.value = response.data.bookingDetail\n      } catch (error) {\n        console.error(error.response.data)\n      }\n    }\n\n    const employeeInfo = computed(() => {\n      return employeeDetail.value\n    })\n\n    const bookingInfo = computed(() => {\n      return bookingDetail.value\n    })\n\n    /**   \n     * Add fee to a checkin\n     * \n     * @param {number} id\n     * \n     * @return {void}\n     */\n\n    getCheckin()\n\n    return {\n      checkins,\n      checkout,\n      destroy,\n      getCheckin,\n      viewDetail,\n      employeeInfo,\n      bookingInfo\n    }\n  },\n  methods: {\n    addFee(id) {\n      console.log(this.fee[id])\n      try {\n        console.log(id, this.fee[id]);\n        const response = axios.post('checked/addFee', {\n          id: id,\n          fee: this.fee[id]\n        })\n        console.log(response.data);\n        this.getCheckin()\n      } catch (error) {\n        console.error(error.response.data)\n      }\n    }\n  },\n})\n</script>\n\n<style scoped>\n@media (min-width: 426px) {\n  .body {\n    background-color: rgb(240, 240, 240);\n    display: flex;\n    flex-direction: column;\n    padding: 20px;\n    border-radius: 3px;\n    height: 100%;\n  }\n\n\n\n  .title-box {\n    display: flex;\n    justify-content: space-between;\n    margin-bottom: 40px;\n  }\n\n  .title {\n    order: 0;\n    font-weight: bold;\n  }\n}\n\n@media (max-width:768px) {\n\n  .body {\n    background-color: rgb(240, 240, 240);\n    display: flex;\n    flex-direction: column;\n    min-height: 100vh;\n  }\n\n  .submit-button {\n    border: none;\n    background: #93e4c1;\n    color: white;\n    border-top-right-radius: 8px;\n    border-bottom-right-radius: 8px;\n  }\n\n  .fee {\n    width: 100px;\n    border: none;\n    border-top-left-radius: 8px;\n    border-bottom-left-radius: 8px;\n  }\n\n  .title-box {\n    padding: 10px;\n    font-weight: bold;\n  }\n\n  .title {\n    order: 0;\n    font-weight: bold;\n  }\n}\n</style>\n"],"mappings":"AAgEA,SAASA,eAAe,EAAEC,GAAG,EAAEC,QAAO,QAAS,KAAI;AACnD,OAAOC,KAAI,MAAO,WAAU;AAE5B,eAAeH,eAAe,CAAC;EAC7BI,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE,CAAC;IACR;EACF,CAAC;EAEDC,KAAKA,CAAA,EAAG;IAEN,MAAMC,QAAO,GAAIP,GAAG,CAAC,EAAE,CAAC;IACxB;;;;;IAKA,MAAMQ,UAAS,GAAI,MAAAA,CAAA,KAAY;MAC7B,MAAMC,QAAO,GAAI,MAAMP,KAAK,CAACQ,GAAG,CAAC,SAAS;MAC1CH,QAAQ,CAACI,KAAI,GAAIF,QAAQ,CAACL,IAAG;IAC/B;;IAEA;;;;;;;;IAQA,MAAMQ,QAAO,GAAI,MAAAA,CA