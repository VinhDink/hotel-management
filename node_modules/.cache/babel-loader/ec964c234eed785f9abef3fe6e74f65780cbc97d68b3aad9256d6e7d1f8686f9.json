ew',\n\n  setup() {\n    const store = createStore(storeConfig)\n    const username = ref('')\n    const password = ref('')\n    const confirm_password = ref('')\n    const email = ref('')\n    const errors = ref('')\n    const role = ref('')\n    // const test = ref('')\n    /**\n     * Register\n     * \n     * @return {void}\n     */\n    const register = async () => {\n      try {\n        console.log(username.value)\n        const response = await axios.post('register', {\n          username: username.value,\n          password: password.value,\n          confirm_password: confirm_password.value,\n          email: email.value,\n          role: role.value,\n        })\n        store.dispatch('login', { username: username.value, password: password.value })\n        console.log(response.data);\n      }\n      catch (error) {\n        errors.value = error.response.data.message\n        alert(errors.value);\n      }\n    }\n\n    return {\n      username,\n      password,\n      confirm_password,\n      email,\n      errors,\n      register,\n    };\n  }\n})\n</script>\n\n<style scoped>\n.body {\n  background-color: rgb(240, 240, 240);\n  display: flex;\n  flex-direction: column;\n  padding: 100px;\n  border-radius: 3px;\n  min-height: 100vh;\n}\n\n.input-box-radio {\n  display: flex;\n  flex-direction: column;\n  align-items: baseline;\n  width: 250px;\n}\n\n.radio {\n  display: flex;\n  gap: 50px;\n}\n</style>\n"],"mappings":"AA4CA,OAAOA,KAAI,MAAO,WAAU;AAC5B,SAASC,GAAG,EAAEC,eAAc,QAAS,KAAI;AACzC,OAAOC,WAAU,MAAO,UAAU;AAClC,SAASC,WAAU,QAAS,MAAM;AAElC,eAAeF,eAAe,CAAC;EAC7BG,IAAI,EAAE,cAAc;EAEpBC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIH,WAAW,CAACD,WAAW;IACrC,MAAMK,QAAO,GAAIP,GAAG,CAAC,EAAE;IACvB,MAAMQ,QAAO,GAAIR,GAAG,CAAC,EAAE;IACvB,MAAMS,gBAAe,GAAIT,GAAG,CAAC,EAAE;IAC/B,MAAMU,KAAI,GAAIV,GAAG,CAAC,EAAE;IACpB,MAAMW,MAAK,GAAIX,GAAG,CAAC,EAAE;IACrB,MAAMY,IAAG,GAAIZ,GAAG,CAAC,EAAE;IACnB;IACA;;;;;IAKA,MAAMa,QAAO,GAAI,MAAAA,CAAA,KAAY;MAC3B,IAAI;QACFC,OAAO,CAACC,GAAG,CAACR,QAAQ